{
  "name": "website",
  "version": "1.0.0",
  "description": "LU Send Website",
  "parcelIgnore": [
    "/index.cfm*",
    "/website/pages/tiptap.js"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:*",
    "dev:parcel": "PARCEL_WORKER_BACKEND=process npx parcel src/_includes/scripts/tiptap.js --dist-dir dist/pages --no-source-maps",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "preview": "ELEVENTY_PREVIEW=true npm run build",
    "build": "run-s build:*",
    "build:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "stage": "run-s stage:*",
    "stage:parcel": "PARCEL_WORKER_BACKEND=process npx parcel build src/_includes/scripts/tiptap.js --dist-dir dist/pages --no-source-maps --no-scope-hoist",
    "stage:eleventy": "ELEVENTY_ENV=stage npx @11ty/eleventy",
    "clean": "rm -Rf dist",
    "minify": "PARCEL_WORKER_BACKEND=process npx parcel build 'dist/**/*.html' 'dist/**/*.js' --dist-dir dist --no-source-maps --no-scope-hoist",
    "predev": "npm run clean",
    "prebuild": "npm run stage",
    "prestage": "npm run clean",
    "postbuild": "npm run minify",
    "postpreview": "npx serve dist -p 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lusend/website.git"
  },
  "author": "P Christopher Bowers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lusend/website/issues"
  },
  "homepage": "https://github.com/lusend/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.1",
    "@tiptap/core": "^2.0.0-beta.175",
    "@tiptap/extension-blockquote": "^2.0.0-beta.26",
    "@tiptap/extension-bold": "^2.0.0-beta.26",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.26",
    "@tiptap/extension-code": "^2.0.0-beta.26",
    "@tiptap/extension-code-block": "^2.0.0-beta.37",
