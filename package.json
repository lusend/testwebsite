{
  "name": "website",
  "version": "1.0.0",
  "description": "LU Send Website",
  "parcelIgnore": [
    "/index.cfm*"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "build": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "stage": "ELEVENTY_ENV=stage npx @11ty/eleventy",
    "clean": "rm -Rf dist",
    "minify": "npx parcel build 'dist/pages/**/*.html' 'dist/pages/*.html' --dist-dir dist/pages --no-source-maps",
    "predev": "npm run clean",
    "prebuild": "npm run stage",
    "prestage": "npm run clean",
    "postbuild": "npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lusend/website.git"
  },
  "author": "P Christopher Bowers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lusend/website/issues"
  },
  "homepage": "https://github.com/lusend/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@tailwindcss/typography": "^0.5.1",
    "@types/jquery": "^3.5.13",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.2.0",
    "node-fetch": "^2.6.7",
    "parcel": "^2.3.2",
    "parcel-resolver-ignore": "^2.0.0",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.18"
  }
}
